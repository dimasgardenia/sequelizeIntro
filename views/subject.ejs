<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Subject</title>
  </head>
  <body>
    <table border="1">
      <tr>
        <th>Id</th>
        <th>Subject Name</th>
        <th>Teacher name</th>
      </tr>
      <% data.forEach(d =>{ %>
        <tr>
          <td><%= d.id %></td>
          <td><%= d.subject_name %></td>
          <td>
            <% d.Teachers.forEach((teacher) => { %>
              <%= teacher.id %> <%= teacher.first_name %>
              <br>
            <% }) %>
          </td>
          <td><a href="/subject/enrolledstudents/<%= d.id %>"><button type="button" name="button">enrolled students</button></a></td>
        </tr>
        <% }) %>
    </table>
    <br>
    <a href="/"><button type="button" name="button">Back To Home</button></a>
  </body>
</html>
