<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Student</title>
  </head>
  <body>
    <table border="1">
      <% if(err){ %>
          <h2>email already use</h2>
        <% } %>
      <tr>
        <th> ID </th>
        <th> First Name </th>
        <th> Last Name </th>
        <th> Email </th>
      </tr>
      <% data.forEach(d=>{ %>
        <tr>
          <td>
            <%= d.id %>
          </td>
          <td>
            <%= d.first_name %>
          </td>
          <td>
            <%= d.last_name %>
          </td>
          <td>
            <%= d.email %>
          </td>
          <td><a href="/student/edit/<%= d.id %>"><button type="button" name="button">Edit</button></a></td>
          <td><a href="/student/delete/<%= d.id %>"><button type="button" name="button">Delete</button></a></td>
          <td><a href="/student/addsubject/<%= d.id %>"><button type="button" name="button">Add Subject</button></a></td>
        </tr>
        <% }) %>
    </table>
    <a href="/student/add">add student</a>
    <br>
  </body>
</html>
